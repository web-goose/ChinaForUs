{% extends "partials/base.html.twig" %}

{% if page.header.isNotIndex %}
    {% block stylesheets %}
        {{ parent() }}
        {% do assets.addCss('theme://css/other-page.css', 101) %}
    {% endblock %}
{% endif %}

{% block content %}
    {% for module in page.collection() %}
        {{ module.content }}
    {% endfor %}
{% endblock %}
